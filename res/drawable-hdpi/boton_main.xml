<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
   <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="#12323d" />
      <stroke
          android:width="1dp"
          android:color="#12323d" />
      <corners
          android:radius="4dp" />
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
    </shape>
  </item>
  <item>
    <shape>
      <gradient
          android:startColor="#423d3d"
          android:endColor="#12323d"
          android:angle="270" />
      <stroke
          android:width="1dp"
          android:color="#423d3d" />
      <corners
          android:radius="4dp" />
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
    </shape>
  </item>
</selector>
